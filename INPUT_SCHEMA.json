{
    "title": "YouTube & Rumble View Bot (test1111 fingerprinting)",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
        "videoUrls": {
            "title": "Video URLs",
            "type": "array",
            "description": "List of YouTube or Rumble video URLs to process",
            "editor": "stringList"
        },
        "watchTimePercentage": {
            "title": "Watch Time Percentage",
            "type": "integer",
            "description": "Percentage of video to watch (1-100)",
            "minimum": 1,
            "maximum": 100,
            "default": 80
        },
        "useProxies": {
            "title": "Use Apify Proxies",
            "type": "boolean",
            "description": "Whether to use Apify proxy service",
            "default": true
        },
        "proxyGroups": {
            "title": "Proxy Groups",
            "type": "array",
            "description": "Proxy groups to use (RESIDENTIAL or DATACENTER)",
            "editor": "select",
            "default": ["RESIDENTIAL"]
        },
        "proxyCountry": {
            "title": "Proxy Country",
            "type": "string",
            "description": "Country code for proxies (e.g., 'US', 'GB')",
            "default": "US"
        },
        "headless": {
            "title": "Headless Mode",
            "type": "boolean",
            "description": "Run browser in headless mode",
            "default": true
        },
        "autoSkipAds": {
            "title": "Auto Skip Ads",
            "type": "boolean",
            "description": "Automatically skip advertisements",
            "default": true
        },
        "skipAdsAfter": {
            "title": "Skip Ads After (seconds)",
            "type": "array",
            "description": "Range of seconds to wait before skipping ads [min, max]",
            "default": [5, 10]
        },
        "useAV1": {
            "title": "Use AV1 Codec",
            "type": "boolean",
            "description": "Use AV1 video codec if available",
            "default": false
        },
        "timeout": {
            "title": "Timeout (seconds)",
            "type": "integer",
            "description": "Timeout for operations in seconds",
            "default": 120
        },
        "concurrencyInterval": {
            "title": "Interval Between Videos (seconds)",
            "type": "integer",
            "description": "Seconds to wait between processing videos",
            "default": 1
        },
        "searchKeywordsForEachVideo": {
            "title": "Search Keywords",
            "type": "array",
            "description": "Keywords to use when searching for videos",
            "editor": "stringList"
        }
    },
    "required": ["videoUrls"]
}
